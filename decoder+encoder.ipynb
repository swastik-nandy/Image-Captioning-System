40	'    print(" Encoder weights loaded with strict=True")\n'
41	"except RuntimeError as e:\n"
42	'    print(" Strict loading failed:", e)\n'
43	'    print(" Retrying with strict=False")\n'
44	'    encoder.load_state_dict(torch.load("encoder_epoch_50.pt", map_location=device), strict=False)\n'
45	"\n"
46	'print(f" Encoder ready (feature dim = {feature_dim})")\n'
